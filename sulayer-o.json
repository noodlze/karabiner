{
    "title": "Sublayer\"o\" (open)",
    "rules": [
        {
            "description": "Sublayer\"o\" (open)",
            "manipulators": [
                {
                    "description": "Open -a 'Safari.app'",
                    "type": "basic",
                    "from": {
                        "key_code": "b",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a 'Safari.app'"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "multitouch_extension_finger_count_total",
                            "value": 4
                        }
                    ]
                },
