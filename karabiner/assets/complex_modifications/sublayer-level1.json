{
  "title": "Sublayer 1",
  "rules": [
    {
      "description": "Sublayer 1",
      "manipulators": [
        {
          "description": "Toggle on/off Layer 1 variable",
          "type": "basic",
          "from": {
            "key_code": "left_option"
          },
          "parameters": {
            "basic.to_if_held_down_threshold_milliseconds": 300
          },
          "to_if_held_down": [
            {
              "set_notification_message": {
                "id": "layer_1_on",
                "text": "LAYER 1 enabled"
              }
            },
            {
              "set_variable": {
                "name": "is_layer_1_on",
                "value": 1
              }
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "is_layer_1_on",
                "value": 0
              }
            },
            {
              "set_notification_message": {
                "id": "layer_1_on",
                "text": ""
              }
            }
          ]
        },
        {
          "description": "Window: Left Half",
          "from": {
            "key_code": "r"
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": ["left_option", "right_command", "right_shift"]
            }
          ],
          "type": "basic",
          "conditions": [
            {
              "type": "variable_if",
              "name": "is_layer_1_on",
              "value": 1
            },
            {
              "type": "variable_if",
              "name": "is_layer_2_on",
              "value": 0
            },
            {
              "type": "variable_if",
              "name": "is_layer_3_on",
              "value": 0
            },
            {
