{
    "title": "Sublayer \"w\"",
    "rules": [
        {
            "description": "Sublayer \"w\"(window)",
            "manipulators": [
                {
                    "description": "Window: Hide",
                    "type": "basic",
                    "from": {
                        "key_code": "h"
                    },
                    "to": [
                        {
                            "key_code": "h",
                            "modifiers": [
                                "right_command"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "multitouch_extension_finger_count_total",
                            "value": 3
                        }
                    ]
                },
                {
                    "description": "Window: First Third",
                    "type": "basic",
                    "from": {
                        "key_code": "j"
                    },
                    "to": [
                        {
                            "key_code": "left_arrow",
                            "modifiers": [
                                "right_option",
                                "control"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "multitouch_extension_finger_count_total",
                            "value": 3
                        }
                    ]
                },
                {
                    "description": "Window: Last Third",
                    "type": "basic",
                    "from": {
                        "key_code": "k"
                    },
                    "to": [
                        {
                            "key_code": "right_arrow",
                            "modifiers": [
                                "right_option",
                                "control"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "multitouch_extension_finger_count_total",
                            "value": 3
                        }
                    ]
                },
                {
                    "description": "Window: Left Half",
                    "type": "basic",
                    "from": {
                        "key_code": "l"
                    },
                    "to": [
                        {
                            "key_code": "left_arrow",
                            "modifiers": [
                                "right_option",
                                "right_command",
                                "right_shift"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "multitouch_extension_finger_count_total",
                            "value": 3
                        }
                    ]
                },
                {
                    "description": "Window: Right Half",
                    "type": "basic",
                    "from": {
                        "key_code": "semicolon"
                    },
                    "to": [
                        {
                            "key_code": "right_arrow",
                            "modifiers": [
                                "right_option",
                                "right_command",
                                "right_shift"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "multitouch_extension_finger_count_total",
                            "value": 3
                        }
                    ]
                },
                {
                    "description": "Window: Full Screen",
                    "type": "basic",
                    "from": {
                        "key_code": "f"
                    },
                    "to": [
                        {
                            "key_code": "f",
                            "modifiers": [
                                "right_option",
                                "right_command"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "multitouch_extension_finger_count_total",
                            "value": 3
                        }
                    ]
                },
                {
                    "description": "Window: Move to next monitor",
                    "type": "basic",
                    "from": {
                        "key_code": "n"
                    },
                    "to": [
                        {
                            "key_code": "n",
                            "modifiers": [
                                "right_option",
                                "right_command",
                                "right_shift"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "multitouch_extension_finger_count_total",
                            "value": 3
                        }
                    ]
                },
                {
                    "description": "Window: Previous Tab",
                    "type": "basic",
                    "from": {
                        "key_code": "u"
                    },
                    "to": [
                        {
                            "key_code": "tab",
                            "modifiers": [
                                "control",
                                "right_shift"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "multitouch_extension_finger_count_total",
                            "value": 3
                        }
                    ]
                },
                {
                    "description": "Window: Next Tab",
                    "type": "basic",
                    "from": {
                        "key_code": "i"
                    },
                    "to": [
                        {
                            "key_code": "tab",
                            "modifiers": [
                                "control"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "multitouch_extension_finger_count_total",
                            "value": 3
                        }
                    ]
                },
                {
                    "description": "next pane (iTerm)",
                    "type": "basic",
                    "from": {
                        "key_code": "comma"
                    },
                    "to": [
                        {
                            "key_code": "left_arrow",
                            "modifiers": [
                                "right_command",
                                "right_option"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "multitouch_extension_finger_count_total",
                            "value": 3
                        }
                    ]
                },
                {
                    "description": "previous pane (iTerm)",
                    "type": "basic",
                    "from": {
                        "key_code": "period"
                    },
                    "to": [
                        {
                            "key_code": "right_arrow",
                            "modifiers": [
                                "right_command",
                                "right_option"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "multitouch_extension_finger_count_total",
                            "value": 3
                        }
                    ]
                }
            ]
        }
    ]
}